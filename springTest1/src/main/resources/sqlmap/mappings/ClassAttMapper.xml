<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="classAtt">

	<resultMap type="map" id="classAttResult">
	<id property="name" column="name"/>
 	</resultMap>
 	
 	
 	


	
	
	
	<select id="getAttList" resultMap="classAttResult" parameterType="String">
	
		select m.name
		from class_info ci join member m
		on(ci.member_id = m.member_id)
		where ci.course_type_cd=#{class_cd}
	
	</select>
	

	

</mapper>